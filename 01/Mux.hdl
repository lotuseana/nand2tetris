/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Or(a=sel, b=a, out=selOrA);
    Not(in=sel, out=notSel);
    Or(a=notSel, b=b, out=notSelOrB);
    And(a=selOrA, b=notSelOrB, out=out);
}